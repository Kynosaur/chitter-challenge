<ul id='messages'>
  <% @messages.reverse.each do |message| %>
    <li>
      <p>
        <span style='border:1px black solid'>
          <%= message.body %>
        </span>
      </p>
    </li>
  <% end %>
</ul>

<form action='new_peep' method='GET'>
  <input type='submit' value='Post new Peep'>
</form>
